{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol Server with Ollama Integration",
  "main": "dist/server.js",
  "scripts": {
    "start:dev": "NODE_ENV=development pnpm build && pnpm start:ollama & sleep 3 && node dist/server.js",
    "start:prod": "NODE_ENV=production pnpm build && node dist/server.js",
    "start:ollama": "ollama serve",
    "test:connection": "ts-node ./test-ollama-connection.ts",
    "test:websocket": "ts-node ./test-websocket-client.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "mcp",
    "ollama",
    "ai",
    "websocket",
    "unity"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "uuid": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "packageManager": "pnpm@10.4.1"
}